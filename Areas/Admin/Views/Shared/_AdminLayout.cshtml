<body class="template-index belle home8-jewellery">
    <div class="pageWrapper">
       
        <!--Top Header-->
        <div class="top-header home8-jewellery-top">
            <div class="container-fluid">
                <div class="row" >
                    <div class="col-sm-12 col-md-12 col-lg-12 d-none d-lg-none d-md-block d-lg-block" >
                        <div class="text-center"><p class="top-header_middle-text"> Quản Lý Hệ Thống</p></div>
                    </div>
                </div>
            </div>
        </div>
        <!--End Top Header-->
        <!--Header-->
        <div class="header-wrap animated d-flex border-bottom">
            <div class="container-fluid">
                <div class="row align-items-center">
                    <!--Desktop Logo-->
                    <div class="logo col-md-2 col-lg-2 d-none d-lg-block">
                        <a href="~/Admin/HomeAdmin/Index">
                            <img src="~/ASSETS/admin/images/admin-login-sign-made-wood-table_292052-431.jpg" alt="Click để trở lại trang chủ" title="Click để trở lại trang chủ" />
                        </a>
                    </div>
                    <!--End Desktop Logo-->
                    <div class="col-2 col-sm-3 col-md-3 col-lg-9">
                        <div class="d-block d-lg-none">
                            <button type="button" class="btn--link site-header__menu js-mobile-nav-toggle mobile-nav--open">
                                <i class="icon anm anm-times-l"></i>
                                <i class="anm anm-bars-r"></i>
                            </button>
                        </div>
                        <!--Desktop Menu-->
                        <nav class="grid__item" id="AccessibleNav">
                            <!-- for mobile -->
                            <ul id="siteNav" class="site-nav medium center hidearrow">
                                <li class="lvl1">
                                    <a href="/Admin/HomeAdmin">Trang chủ <i class="anm anm-angle-down-l"></i></a>
                                </li>
                                <li class="lvl1 ">
                                    <a href="@Url.Action("Index", null, new { area = "Admin", controller = "CategoryAdmin"}, Request.Url.Scheme)">Danh mục <i class="anm anm-angle-down-l"></i></a>
                                </li>
                                <li class="lvl1 ">
                                    <a href="@Url.Action("Index", null, new { area = "Admin", controller = "UserData"}, Request.Url.Scheme)">Dữ liệu người dùng<i class="anm anm-angle-down-l"></i></a>
                                </li>
                                <li class="lvl1 ">
                                    <a href="@Url.Action("Index", null, new { area = "Admin", controller = "OrderAdmin"}, Request.Url.Scheme)">Đơn hàng <i class="anm anm-angle-down-l"></i></a>
                                </li>
                                <li class="lvl1">
                                    <a href="@Url.Action("Index", null, new { area = "Admin", controller = "ProductAdmin"}, Request.Url.Scheme)"><b>Sản phẩm</b> <i class="anm anm-angle-down-l"></i></a>
                                </li>
                            </ul>
                        </nav>
                        <!--End Desktop Menu-->
                    </div>
                    <!--Mobile Logo-->
                    <div class="col-4 col-sm-3 col-md-3 col-lg-1">
                        <div class="header-button">                          
                            <div class="account-wrap">
                                <div class="account-item clearfix js-item-menu">
                                    <div class="image">
                                        <img src="/Asset/admin/images/icon/avatar-01.jpg" alt="John Doe" />
                                    </div>
                                    <div class="content">
                                        <a class="js-acc-btn" href="#">Admin</a>
                                    </div>
                                    <div class="account-dropdown js-dropdown">
                                        <div class="info clearfix">
                                            <div class="image">
                                                <a href="#">
                                                    <img src="/Asset/admin/images/icon/avatar-01.jpg" alt="John Doe" />
                                                </a>
                                            </div>
                                            <div class="content">
                                                <h5 class="name">
                                                    <a href="#">Quản trị viên</a>
                                                </h5>
                                                <span class="email">admin@gmail.com</span>
                                            </div>
                                        </div>
                                        <div class="account-dropdown__body">
                                            <div class="account-dropdown__item">
                                                <a href="@Url.Action("UserInfor", null, new { area = "Admin", controller = "UserData"}, Request.Url.Scheme)">
                                                    <i class="zmdi zmdi-account"></i>Tài khoản
                                                </a>
                                            </div>
                                            <div class="account-dropdown__item">
                                                <a href="#">
                                                    <i class="zmdi zmdi-settings"></i>Cài đặt
                                                </a>
                                            </div>
                                            <div class="account-dropdown__item">
                                                <a href="#">
                                                    <i class="zmdi zmdi-money-box"></i>aa
                                                </a>
                                            </div>
                                        </div>
                                        <div class="account-dropdown__footer">
                                            <a href="/Login/Login">
                                                <i class="zmdi zmdi-power"></i>Đăng xuất
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--End Header-->
        <!--Body Content-->
        <div id="page-content">

            @RenderBody()
        </div>
        <!--End Body Content-->
        <!--Footer-->
        <footer id="footer" class="footer-3">
            <div class="site-footer">
                <div class="container">
                    <hr>
                    <div class="footer-bottom">
                        <div class="row">
                            <!--Footer Copyright-->
                            <div class="col-12 col-sm-12 col-md-12 col-lg-12 copyright text-center"><span></span> <a href="/Admin/HomeAdmin/Index">Trang chủ</a></div>
                            <!--End Footer Copyright-->
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!--End Footer-->
        <!--Scoll Top-->
        <span id="site-scroll"><i class="icon anm anm-angle-up-r"></i></span>
        <!--End Scoll Top-->   
     
        <!-- Including Jquery -->
        <script src="~/Asset/assets/js/vendor/jquery-3.3.1.min.js"></script>
